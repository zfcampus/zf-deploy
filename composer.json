{
    "name": "zfcampus/zf-deploy",
    "description": "Deployment tool for Zend Framework applications",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "zend",
        "apigility"
    ],
    "homepage": "http://framework.zend.com/",
    "support": {
        "source": "https://github.com/zfcampus/zf-deploy",
        "issues": "https://github.com/zfcampus/zf-deploy/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev",
            "dev-develop": "1.3-dev"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "herrera-io/phar-update": "~1.0",
        "zendframework/zend-filter": "^2.7.1",
        "zfcampus/zf-console": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "ZF\\Deploy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\Deploy\\": "test/"
        }
    },
    "bin": [
        "bin/zfdeploy.php"
    ],
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
