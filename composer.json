{
    "name": "zfcampus/zf-deploy",
    "description": "Deployment tool for ZF2 application",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "zend",
        "apigility"
    ],
    "homepage": "http://framework.zend.com/",
    "support": {
        "source": "https://github.com/zfcampus/zf-deploy",
        "issues": "https://github.com/zfcampus/zf-deploy/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "herrera-io/phar-update": "~1.0",
        "zendframework/zend-console": "~2.0",
        "zendframework/zend-filter": "~2.0",
        "zfcampus/zf-console": "~1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "*@dev",
        "phpunit/PHPUnit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "ZF\\Deploy\\": "src/"
        }
    },
    "bin": ["bin/zfdeploy.php"]
}
